<script webc:setup>
  function allPhotos() {
    return Object.values(this.$data.photos)
      .filter((photo) => photo.tags?.includes('limulus.net/photos'))
      .sort((a, b) => b.date.localeCompare(a.date))
  }
</script>

<h1 @text="title"></h1>

<photo-list :@photos="allPhotos()"></photo-list>
