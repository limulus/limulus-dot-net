    "cdk-nag": "^2.36.50",
    "constructs": "^10.4.2",
    "dotenv": "^16.4.7",
    "dprint": "^0.50.2",
    "dprint-plugin-markup": "^0.24.0",
    "esbuild-plugin-wasm": "^1.1.0",
    "globby": "^14.0.1",
    "husky": "^9.1.5",
    "patch-package": "^8.0.1",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  }
}
