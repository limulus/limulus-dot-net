    "del-cli": "^5.1.0",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "exifreader": "^4.25.0",
    "highlight.js": "^11.9.0",
    "hls.js": "^1.5.18",
    "into-stream": "^8.0.1",
    "listr": "^0.14.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
